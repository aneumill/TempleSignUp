@{
    ViewData["Title"] = "SignUp Page";
}

@model IEnumerable<SlottedTime>;

<div>

    <h1 class="text-center">Available Tour Times</h1>
    <table class="center table table-light table-striped table-hover table-bordered">
        <!--Table for the database-->
        <thead>
            <tr>
                <th>Date </th>
                <th>Time</th>
                <th>Book</th>

            </tr>
        </thead>



        <tbody>
            <!--Loops through all of the items in the database to print them out here-->
            @foreach (var x in Model)
            {

                <partial name="ViewAppointmentsSummary" model="x" />

             }
</tbody>
    </table>
</div>