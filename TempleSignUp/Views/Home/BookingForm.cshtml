@{
    ViewData["Title"] = "Booking Form";
    
    
}
@model TempleSignUp.Models.SignUpModel

<form asp-action="BookingForm" method="post" class="center">
    <div>
        <label asp-for="GroupName">Group Name</label>
        <input asp-for="GroupName" placeholder="" />
    </div>
    <div>
        <label asp-for="GroupSize">Group Size</label>
        <input placeholder="" asp-for="GroupSize" />
    </div>
    <div>
        <label asp-for="Email">email</label>
        <input asp-for="Email" placeholder="" />
    </div>
    <div>
        <label asp-for="PhoneNumber">Phone</label>
        <input placeholder="801-111-1111" type="text" asp-for="PhoneNumber" />
    </div>
    <br />
    <button type="submit" id="button" class="btn btn-info"> Submit </button>
</form>

<div asp-validation-summary="All"></div>